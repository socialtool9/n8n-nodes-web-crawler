  },
  "dependencies": {
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "mysql2": "^3.9.1",
    "pg": "^8.11.3",
    "image-size": "^1.1.1",
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@types/pg": "^8.10.9",
    "@types/image-size": "^0.8.0",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.5"
  }
}
